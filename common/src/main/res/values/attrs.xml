<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SexView">
        <attr name="maleColor" format="color"/>
        <attr name="femaleColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="CircleViewPager">
        <!--选中时的圆点图片-->
        <attr name="lightDotRes" format="reference"/>
        <!--未选中时的圆点图片-->
        <attr name="darkDotRes" format="reference"/>
        <!--圆点半径-->
        <attr name="dotWidth" format="dimension"/>
        <!--页面切换时间间隔-->
        <attr name="interval" format="integer"/>

    </declare-styleable>
</resources>